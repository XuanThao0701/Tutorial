#include <iostream>

using namespace std;

int N;
int A[10000];

void convertArray(int A[10000], int length)
{
    for (int i = length - 1; i >= 0; i--)
    {
        printf("%d ", A[i]);
    }
}

int main()
{
    scanf("%d", &N);
    for (int i = 0; i < N; i++)
    {
        scanf("%d", &A[i]);
    }

    convertArray(A, N);
}